{
  "name": "nodeunit-browser-tap",
  "version": "0.0.1",
  "description": "Nodeunit browser-side TAP reporter for use with Testling CI",
  "main": "lib/reporter.js",
  "scripts": {
    "test": "./node_modules/.bin/browserify test/reporter.js | cat lib/nodeunit.js - | ./node_modules/.bin/testling"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hurrymaplelad/nodeunit-browser-tap"
  },
  "keywords": [
    "nodeunit",
    "browser",
    "tap",
    "testling"
  ],
  "author": "Adam Hull <adam@hmlad.com>",
  "license": "MIT",
  "dependencies": {
    "tap-assert": "0.0.x",
    "tap-producer": "0.0.x"
  },
  "devDependencies": {
    "testling": "1.x",
    "browserify": "2.x",
    "sinon": "1.6.x"
  },
  "testling": {
    "scripts": "lib/nodeunit.js",
    "files": "test/reporter.js",
    "browsers": {
      "chrome": [
        25,
        "canary"
      ],
      "ie": [
        8,
        9,
        10
      ],
      "firefox": [
        19,
        "nightly"
      ],
      "opera": [
        12
      ],
      "safari": [
        5.1,
        6
      ],
      "iphone": [
        6
      ],
      "android-browser": [
        4.2
      ]
    }
  }
}
